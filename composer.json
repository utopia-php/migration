{
    "name": "utopia-php/migration",
    "description": "A simple library to migrate resources between services.",
    "type": "library",
    "keywords": ["php", "framework", "upf", "utopia", "migration"],
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Utopia\\Migration\\": "src/Migration"
        }
    },
    "autoload-dev": {
        "psr-4": {"Utopia\\Tests\\": "tests/Migration"}
    },
    "scripts": {
        "lint": "./vendor/bin/pint --test",
        "format": "./vendor/bin/pint"
    },
    "require": {
        "php": "8.*",
        "utopia-php/cli": "0.*",
        "appwrite/appwrite": "10.0.*",
        "utopia-php/cache": "^0.8.0",
        "utopia-php/logger": "^0.4.0",
        "barryvdh/laravel-ide-helper": "^2.13"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "vlucas/phpdotenv": "5.*",
        "laravel/pint": "1.*",
        "ukko/phpredis-phpdoc": "dev-master"
    }
}
